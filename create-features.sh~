export msisdn="${msisdn:-"491720451021"}"
export email="${email:-"aiosamy15@gmail.com"}"
export pass="${pass:-"Keethan12"}"
export picname="${picname:-"Buddha8.jpeg"}"
for file in features/templates/*.tpl
do
	basename=$(basename "$file")
	name="${basename%.*}"
	sed 's/<msisdn>/'"$msisdn"'/g
	sed 's/<email>/'"$email"'/g
	sed 's/<pass>/'"$pass"'/g
	sed 's/<picname>/'"$picname"'/g' $file >features/$name.feature
done
